{
  "name": "the-orangee-dictionary",
  "version": "1.0.4",
  "description": "A multilingual dictionary lib",
  "author": "Renata Rocha & Phabiulla Nascimento",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "generate-types": "tsc --declaration --outDir lib/ --emitDeclarationOnly --declarationMap",
    "build": "babel src --out-dir dist --extensions \".ts\" && npm run generate-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reehlittle/the-orangee-dictionary.git"
  },
  "bugs": {
    "url": "https://github.com/reehlittle/the-orangee-dictionary/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "homepage": "https://github.com/reehlittle/the-orangee-dictionary#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0"
  }
}
